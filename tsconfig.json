{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "declaration": true,
    "outDir": "dist"
  },
  "include": ["./*.ts", "./*.tsx", "src/index.ts"],
  "exclude": ["node_modules", "dist"]
}
